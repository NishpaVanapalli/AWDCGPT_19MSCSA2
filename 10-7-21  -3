import java.util.*;
class oddeven_wise
{
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a string");
		String st=sc.next();
		int n=st.length();
		String even=new String();
		String odd=new String();
		int i, j;
		for(i=0;i<n;i++)
		{
			j=i+1;
			if(j%2==0)
			even+=st.charAt(i);
			else
			odd+=st.charAt(i);
		}
	System.out.print(odd+"  "+even);
	}
}


o/p 1:
Enter a string
123456
135  246

o/p 2:
Enter a string
ABCD
AC  BD
