import java.util.*;
class smallfact
{
	public int fact(int a[j], int n)
	{
		int j, f=1, k;
		for(k=a[j];k>0;k--)
			f*=k;
		return f;
	}
	public static void main(String ar[])
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the number of test cases");
		int t=sc.nextInt();
		int a[]=new int[t];
		int b[]=new int[t];
		int i, j;
		
		System.out.println("Enter "+ t +" values");
		for(i=0;i<t;i++)
			a[i]=sc.nextInt();
  
        	for (j=0;j<t;j++)
        	{
            		b[j]=fact(a[j],t);
        	}
		
		for(j=0;j<t;j++)
        		System.out.println(b[j]);
		return;
	}
}


o/p :
Enter the number of test cases
4
Enter 4 values
1
2
5
3
1
