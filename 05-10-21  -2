import java.util.*;
class fib_n
{
	public static void main(String args[])
	{
		Scanner sc = new Scanner(System.in);
		int a=0, b=1, c=0;
		System.out.print("Enter the n value : ");
		int n = sc.nextInt();
		if(n == 0 || n == 1) 
			System.out.println(n);
		else
			c = a + b;
		for (int i = 3; i <= n; ++i)
		{
			a = b;
			b = c;
			c = a+b;
		}
	System.out.println("Sum is : "+b);
	}
}



o/p:
Enter the n value : 5
Sum is : 3
