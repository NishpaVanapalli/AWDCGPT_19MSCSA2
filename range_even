import java.util.*;
class range_even
{
	public static void main(String ar[])
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter m value");
		int m=sc.nextInt();
		System.out.println("Enter n value");
		int n=sc.nextInt();
		while(m>=n)
		{
			System.out.println(m);
			m-=2;
		}
	}
}

o/p:
Enter m value
50
Enter n value
10
