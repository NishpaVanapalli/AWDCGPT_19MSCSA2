import java.util.*;
class pattern
{
public static void main(String ar[])
{
Scanner sc=new Scanner(System.in);
System.out.println("Enter number of test cases");
int t=sc.nextInt();
System.out.println("Enter N value");
int N=sc.nextInt();
int val=0;

for(int i=0;i<N;i++)
{
val=1;
for(int j=1;j<=N;j++)
{
System.out.print(val+" ");
val+=i+j;
}
System.out.println();

}
}
}


o/p :
Enter number of test cases
1
Enter N value
4
1 2 4 7
1 3 6 10
1 4 8 13
1 5 10 16
