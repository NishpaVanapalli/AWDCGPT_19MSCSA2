import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int count=0;
        for(int i=n;i>1;i--)
        {
            if(n%i==0)
                count++;
        }
        if(count==1)
            System.out.print("yes");
        else
            System.out.print("no");
    }
}


